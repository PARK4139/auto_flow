      document.addEventListener('DOMContentLoaded', function () {
            document.addEventListener('keydown', function (event) {
                // Alt 키가 눌렸을 때만 동작
                if (event.altKey) {
                    const key = event.key.toLowerCase();

                    switch (key) {
