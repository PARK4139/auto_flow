     <script>
      // JavaScript DOM Null 참조 방지
        $(function () {
            var $aboutDiv = $('.main_board2');  // 게시판
            if ($aboutDiv.length === 0) return;  // 요소가 없으면 종료

            var $btnOb = $aboutDiv.find('.nav-tabs').children();  // 탭버튼
            var timerSpeed = 6000;  // 타이머 6초
            var aboutTimer = null;
            var btnNum = $aboutDiv.find('li a').length;  // 탭 갯수

            if (btnNum === 0) return;  // 탭이 없으면 종료

            startAboutTimer();  // 타이머 시작

            $aboutDiv.mouseleave(function (e) { startAboutTimer(); });  // 마우스가 게시판 밖으로 나가면 타이머 시작
            $aboutDiv.mouseover(function (e) { stopAboutTimer(); });  // 마우스가 게시판 안으로 들어오면 타이머 멈춤

            function startAboutTimer() {
                if (aboutTimer) clearInterval(aboutTimer);  // 중복 타이머 방지
                aboutTimer = setInterval(btnTimer, timerSpeed);
            }
            function stopAboutTimer() { clearInterval(aboutTimer); aboutTimer = null; }

            // 타이머
            function btnTimer() {
                var $activeTab = $aboutDiv.find('li.active');
                if ($activeTab.length === 0) return;

                var operateBtnNum = $activeTab.index(); // 활성중인 탭 index

                operateBtnNum++;  // 다음 탭

