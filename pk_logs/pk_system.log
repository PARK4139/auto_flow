var $url_split = $(location).attr('search').split('&');
var $url_mode = $url_split[0].replace('?mode=','');


var arr_int_idx = new Array('3', '11', '17', '34', '35', '116'); // 메뉴 추가시 배열에 값 추가

for (i = 0; i < arr_int_idx.length; i++) {
    Callajax(arr_int_idx[i]);
}

function Callajax(int_idx) {
	$.ajax({
	    type: "post",
	    url: "/board/board_tree_xml.aspx",
	    data: { "mode": int_idx },
	    //var allData = { "userId": userId, "checkArray": checkboxValues };
	    dataType: "xml",
	    success: function (result) {
		var board_tree_str = '';

		$(result).find(".depth1").each(function () {
			board_tree_str += '<li class="mode' + $(this).attr("id") +' img-minus"><a href="/board/board_list.aspx?mode=' + $(this).attr("id") +'&page=1">' + $(this).attr("text") + '</a>';
			var z = 0;
			if (z == 0) {
			    board_tree_str += '<ul style="display:block;">';
			}
			$(this).find(".depth2").each(function () {
			    board_tree_str += '<li class="mode' + $(this).attr("id") +'"><a href="/board/board_list.aspx?mode=' + $(this).attr("id") +'&page=1">' + $(this).attr("text") + '</a>';
			    var i = 0;
			    $(this).find(".depth3").each(function () {
				if (i == 0) {
				    board_tree_str += '<ul>';
				}
				board_tree_str += '<li class="mode' + $(this).attr("id") +'"><a href="/board/board_list.aspx?mode=' + $(this).attr("id") +'&page=1">' + $(this).attr("text") + '</a>';

				var j = 0;
				$(this).find(".depth4").each(function () {
				    if (j == 0) {
					board_tree_str += '<ul>';
				    }
				    board_tree_str += '<li class="mode' + $(this).attr("id") +'"><a href="/board/board_list.aspx?mode=' + $(this).attr("id") +'&page=1" class="lnb_5da">' + $(this).attr("text") + '</a>';

				    var k = 0;
				    $(this).find(".depth5").each(function () {
					if (k == 0) {
					    board_tree_str += '<ul>';
					}
					board_tree_str += '<li class="mode' + $(this).attr("id") +'"><a href="/board/board_list.aspx?mode=' + $(this).attr("id") +'&page=1" class="lnb_6da">' + $(this).attr("text") + '</a></li>';
					k++;
				    });
				    if (k > 0) {
					board_tree_str += '</ul>';
				    }
				    board_tree_str += '</li>';
				    j++;
				});
				if (j > 0) {
				    board_tree_str += '</ul>';
				}
				board_tree_str += '</li>';
				i++;
			    });
			    if (i > 0) {
				board_tree_str += '</ul>';
			    }
			    board_tree_str += '</li>';
			    z++;
			});
			if (z > 0) {
			    board_tree_str += '</ul>';
			}
			board_tree_str += '</li>';
		});


		$(".tree_menu_"+int_idx).html('<ul>'+board_tree_str+'</ul>');

		//트리메뉴 시작
		var treeBtn=$(".tree_menu_"+int_idx+" a");
		var treeLi=$(".tree_menu_"+int_idx+" li");
		var firstLi=$(".tree_menu_"+int_idx+">ul>li:first-child");
		var lastLi=$(".tree_menu_"+int_idx+" ul li:last-child");
		var lastUl=lastLi.children("ul");

		firstLi.addClass("first");
		lastLi.addClass("last");
		lastUl.addClass("last");

		for(var i=0; i<treeLi.length; i++){
		var thisLi=treeLi.eq(i);
		var haveUl=thisLi.children("ul");
		var haveUlNum=haveUl.length;
		var thisbg=thisLi.attr("class");

			if(haveUlNum=="0"){
				if(thisbg=="first"){
					 thisLi.addClass("none_first");
				}else if(thisbg=="last"){
					thisLi.addClass("none_last");
				}else{
					thisLi.addClass("none");
				}
			}else if(haveUlNum!="0"){
				thisLi.children("a").before("<img src='/images/treemenu/btn_plus.gif' alt='하위메뉴 보기' />");
			}
		}

		var treeBtn=$(".tree_menu_"+int_idx+" li img");

		treeBtn.on('click', function(){
			var thisNum=treeBtn.index(this);
			var thisBtn=treeBtn.eq(thisNum);
			var thisSubUl=thisBtn.parent().children("ul");

			if(thisSubUl.is(":visible")){
				thisBtn.attr("src",thisBtn.attr("src").replace("minus.gif","plus.gif"));
				thisSubUl.slideUp("500");
			}else{
				thisBtn.attr("src",thisBtn.attr("src").replace("plus.gif","minus.gif"));
				thisSubUl.slideDown("500");
			}


		})
		//트리메뉴 끝
        $('.mode65').attr('onclick', 'javascript:alert("폐지된 문서 입니다.\\n효력을 상실하였기 때문에 단순히 참고만 하십시오.");');
        $('.mode69').attr('onclick', 'javascript:alert("폐지된 문서 입니다.\\n효력을 상실하였기 때문에 단순히 참고만 하십시오.");');
		$('.mode'+$url_mode).addClass('active');
		$('.mode'+$url_mode).parents('.treeMenu').addClass('in');
		$('.mode'+$url_mode).parents('ul').css('display','block');
		$('.mode'+$url_mode).parents('li').children('img').attr("src",'/images/treemenu/btn_minus.gif');
		$('.img-minus').children('img').attr("src",'/images/treemenu/btn_minus.gif');


	    },
	    //error: function () {
	    //    alert("오류가 발생하였습니다.");
	    //}
	});
}
