       <script lang="javascript" type="text/javascript">
        var strMinute = '';
		if(!strMinute){
			strMinute = 0;
		}
		var strState = '';
		if(!strState){
			strState = 0;
		}

	    	// 시간 변경
	    	$('.time_now2').html("09:00~18:00");
	    	$('.time_now').html(parseInt(strMinute/60)+'시간'+' '+(strMinute%60)+'분');
	    	
	        // 식사시간 설정
	        function autoRefresh_meal() {
	            var today = new Date();
	            var hours = today.getHours(); // 시간
	            var minutes = today.getMinutes(); // 분
	          if ((hours == 7 && minutes >= 30) || (hours == 8 && minutes < 30) || (hours == 18 && minutes < 30)) {
	          //if ((hours >= 8 && hours <= 22)) {
	                // 활성화
	                if ($('.btn_meal').hasClass('btn_attendance_off')) {
	                    $('.btn_meal').removeClass('btn_attendance_off');
	                }
	            } else {
	                // 비활성화
	                if (!$('.btn_meal').hasClass('btn_attendance_off')) {
	                    $('.btn_meal').addClass('btn_attendance_off');
	                }
	            }
	        }
		//alert(strState);
		if(strState == 0){
			$('.btn_attendance_state_on').addClass('btn_attendance_off');
			$('.btn_attendance_state_off').addClass('btn_attendance_off');
		}else if(strState == 1){
		    if(!$('.btn_attendance_state_on').hasClass('active')){
				$('.btn_attendance_state_on').addClass('active');
			}
			if($('.btn_attendance_state_off').hasClass('active')){
				$('.btn_attendance_state_off').removeClass('active');
			}
		    autoRefresh_meal(); // 초기 실행
		    setInterval('autoRefresh_meal()', 1000); //1초 후 새로고침
		}else if(strState == 2){
		    if($('.btn_attendance_state_on').hasClass('active')){
				$('.btn_attendance_state_on').removeClass('active');
			}
			if(!$('.btn_attendance_state_off').hasClass('active')){
				$('.btn_attendance_state_off').addClass('active');
			}
		}else if(strState == 3){
		    //$('.btn_attendance_state').addClass('btn_attendance_off');
			if($('.btn_attendance_state_on').hasClass('active')){
				$('.btn_attendance_state_on').removeClass('active');
			}
			if(!$('.btn_attendance_state_off').hasClass('active')){
				$('.btn_attendance_state_off').addClass('active');
			}
		}
