pt>\n\n\n    \u003Cscript lang=\"javascript\" type=\"text/javascript\">\n        // JavaScript DOM Null 참조 방지\n        function check() {\n            var form = document.getElementById('fr_search');\n            var searchInput = form ? form.querySelector('input[name=\"main_search\"]') : null;\n            if (!searchInput || searchInput.value === \"\") {\n                alert(\"검색어를 입력해주세요.\");\n                if (searchInput) {\n                    searchInput.focus();\n                }\n                return false;\n            }\n            return true;\n        }\n    \u003C/script>\n    \u003Cspan id=\"ContentPlaceHolder1_lblname\">\u003C/span>\n    \u003Cspan id=\"ContentPlaceHolder1_lbcheck\">\u003C/span>\n\n    \u003C!-- 팝업 쿠키 -->\n    \u003Cscript language=\"javaScript\">\n        // JavaScript DOM Null 참조 방지 및 보안 개선\n        function setCookie(name, value, expiredays) {\n            var todayDate = new Date();\n            todayDate.setDate(todayDate.getDate() + expiredays);\n            // escape() 대신 encodeURIComponent() 사용 (보안 개선)\n            document.cookie = name + \"=\" + encodeURIComponent(value) + \"; path=/; expires=\" + todayDate.toGMTString() + \";\"\n        }\n\n        function closeWin() {\n            var checkedRadio = document.querySelector(\"input[name='pop_radio']:checked\");\n            var pop_select = checkedRadio ? checkedRadio.value : null;\n            if (pop_select == '1') {\n                setCookie(\"maindiv\", \"done\", 1);\n            } else if (pop_select == '2') {\n                setCookie(\"maindiv\", \"done\", 7);\n            }\n            // document.all 대신 getElementById 사용 (표준 DOM API)\n            var divpop = document.getElementById('divpop');\n            if (divpop) {\n                divpop.style.visibility = \"hidden\";\n            }\n        }\n\n        // 레거시 브라우저 감지 코드 제거 (보안 및 호환성 개선)\n        function hide() {\n            var loadElement = document.getElementById('Load');\n            if (loadElement) {\n                loadElement.style.visibility = \"hidden\";\n            }\n        }\n    \u003C/script>\n\n    \u003Cscript type=\"text/javascript\">\n        // JavaScript DOM Null 참조 방지\n        function openPopup(url, idx) {\n            var popupForm = document.forms['popup_form'];\n            if (!popupForm) return;\n\n            var idxInput = popupForm.elements['idx'];\n            if (idxInput) {\n                idxInput.value = idx;\n            }\n            window.open(\"\", 'popOpen', 'fullscreen=no,menubar=no,status=no,toolbar=no,titlebar=no,location=no,scrollbar=auto,width=720,height=725');\n            popupForm.target = \"popOpen\";\n            popupForm.action = url;\n            popupForm.submit();\n        }\n    \u003C/script>\n\n    \u003C!-- 팝업1 시작 -->\n    \u003Cform name=\"popup_form\">\n        \u003Cinput type=\"hidden\" name=\"UserId\" value=\"250037\">\n        \u003Cinput type=\"hidden\" name=\"idx\">\n        \u003Cdiv id=\"divpop\" style=\"color: rgb(255, 255, 255); position: absolute; top: 10px; left: 10px; border: 1px solid rgb(117, 117, 117); padding: 0px; z-index: 9999; visibility: visible;\">\n            \u003Ctable border=\"0\" cellpadding=\"0\" cellspacing=\"0\">\n                \u003Ctbody>\u003Ctr>\n                    \u003Ctd colspan=\"2\">\n                        \u003Cimg src=\"/Images/main/popup_20190625.jpg\" alt=\"\">\u003C/td>\n                \u003C/tr>\n                \u003Ctr>\n                    \u003Ctd bgcolor=\"#000\" style=\"padding: 5px;\">\n                        \u003Clabel for=\"pop_radio1\" style=\"font-weight: 300; cursor: pointer\">\n                            \u003Cinput type=\"radio\" name=\"pop_radio\" id=\"pop_radio1\" value=\"1\" style=\"background: #000; border: 0; vertical-align: -3px;\">&nbsp;하루 다시보지 않기\u003C/label>&nbsp;&nbsp;/&nbsp;&nbsp;\u003Clabel for=\"pop_radio2\" style=\"font-weight: 300; cursor: pointer\">\u003Cinput type=\"radio\" name=\"pop_radio\" id=\"pop_r